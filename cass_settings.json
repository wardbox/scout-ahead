{
    "api_key": "RIOT_API_KEY",
    "global": {
        "version_from_match": "latest",
        "default_region": null
    },
    "limiting_share": 1.0,
    "request_error_handling": {
        "404": {
            "strategy": "throw"
        },
        "429": {
            "service": {
                "strategy": "exponential_backoff",
                "initial_backoff": 1.0,
                "backoff_factor": 2.0,
                "max_attempts": 4
            },
            "method": {
                "strategy": "retry_from_headers",
                "max_attempts": 5
            },
            "application": {
                "strategy": "retry_from_headers",
                "max_attempts": 5
            }
        },
        "500": {
            "strategy": "exponential_backoff",
            "initial_backoff": 1.0,
            "backoff_factor": 2.0,
            "max_attempts": 4
        },
        "503": {
            "strategy": "exponential_backoff",
            "initial_backoff": 1.0,
            "backoff_factor": 2.0,
            "max_attempts": 4
        },
        "timeout": {
            "strategy": "throw"
        }
    },
    "logging": {
        "print_calls": true,
        "print_riot_api_key": false,
        "default": "WARNING",
        "core": "WARNING"
    },
    "pipeline": {
        "Cache": {},

        "SimpleKVDiskStore": {
            "package": "cassiopeia_diskstore",
            "path": "C:/git/scout-ahead/instance/data/"
        },

        "DDragon": {},

        "RiotAPI": {
            "api_key": "RIOT_API_KEY"
        }
    }
}